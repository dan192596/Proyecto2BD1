@model List<List<String>>
@{
    var Empleado = (String)ViewData["empleado"];
    var Nombre = (String)ViewData["nombre"];
    ViewData["Title"] = "Reporte3";
}

<style type="text/css">
    * {
        padding: 0px;
        margin: 0px;
    }

    #header {
        margin: auto;
        width: 600px;
        font-family: Arial, Helvetica, sans-serif;
    }

    ul, ol {
        list-style: none;
    }

    .nav li a {
        background-color: #000000;
        color: #fff;
        text-decoration: none;
        padding: 10px 15px;
        display: block;
    }

    .nav a:hover {
        background-color: #434343;
    }

    .nav > li {
        float: left;
    }

    .nav li ul {
        display: none;
        position: absolute;
        min-width: 140px;
    }

    .nav li:hover > ul {
        display: block;
    }

    .nav li ul li {
        position: relative;
    }

        .nav li ul li ul {
            right: -140px;
            top: 0px;
        }
</style>

<div class="text-right">
    <h3>Catedratico: @Nombre -- Codigo: @Empleado </h3>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="Home" asp-action="Index">Logout</a>
</div>

<div class="home_content text-center">
    <h2>Modulo Administrador</h2>
</div>

<div id="header" class="home_content text-center">
    <ul class="nav">

        <li>
            <a href="">Administrador</a>
            <ul>
                <li><a asp-controller="Administrador" asp-action="Index">Log Actividades</a></li>
                <li><a asp-controller="Administrador" asp-action="Create">Usuarios</a></li>
            </ul>
        </li>

        <li>
            <a href="">Caja </a>
            <ul>
                <li><a href="">Movimiento Diario</a></li>
                <li><a href="">Otros Egresos</a></li>
            </ul>
        </li>

        <li>
            <a href="">Ventas </a>
            <ul>
                <li><a href="">Registrar Venta</a></li>
                <li><a href="">Facturacion</a></li>
                <li><a href="">Reportes</a></li>
            </ul>
        </li>

        <li>
            <a href="">Cliente </a>
            <ul>
                <li><a asp-controller="Cliente" asp-action="Create">Registro Cliente</a></li>
                <li><a href="">Historial Cliente</a></li>
            </ul>
        </li>

        <li>
            <a href="">Categoria </a>
            <ul>
                <li>
                    <a asp-controller="Categoria" asp-action="Create">Registrar Categoria</a>
                    <ul>
                        <li><a asp-controller="Categoria" asp-action="Create">Crear</a></li>
                        <li><a asp-controller="Categoria" asp-action="Edit">Editar</a></li>
                        <li><a asp-controller="Categoria" asp-action="Delete">Eliminar</a></li>
                    </ul>
                </li>
                <li><a asp-controller="Categoria" asp-action="Index">Consultar</a></li>
            </ul>
        </li>

        <li>
            <a href="">Productos </a>
            <ul>
                <li>
                    <a asp-controller="Producto" asp-action="Create">Registrar Producto</a>
                    <ul>
                        <li><a asp-controller="Producto" asp-action="Create">Crear</a></li>
                        <li><a asp-controller="Producto" asp-action="Edit">Editar</a></li>
                        <li><a asp-controller="Producto" asp-action="Delete">Eliminar</a></li>
                        <li><a asp-controller="Descuento" asp-action="Create">Descuento</a></li>
                    </ul>
                </li>
                <li>
                    <a href="">Inventario</a>
                    <ul>
                        <li><a asp-controller="Categoria" asp-action="Index">Productos por categoria</a></li>
                        <li>
                            <a href="">Reportes</a>
                            <ul>
                                <li><a asp-controller="InventarioReporte" asp-action="Reporte1">Productos mas vendidos</a></li>
                                <li><a asp-controller="InventarioReporte" asp-action="Reporte2">Productos sin existencia</a></li>
                                <li><a asp-controller="InventarioReporte" asp-action="Reporte3">Productos mas comprados</a></li>
                                <li><a asp-controller="InventarioReporte" asp-action="Reporte4">Productos con descuento</a></li>
                                <li><a asp-controller="InventarioReporte" asp-action="Reporte5">Mejores Proveedores</a></li>
                                <li><a asp-controller="InventarioReporte" asp-action="Reporte6">Ver todos los productos</a></li>
                            </ul>
                        </li>
                    </ul>
                </li>
                <li><a asp-controller="Compra" asp-action="Create">Compras</a></li>
                <li><a asp-controller="Proveedor" asp-action="Create">Proveedores</a></li>
            </ul>
        </li>
    </ul>
</div>

<h2>Productos mas comprados</h2>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Nombre</td>
            <td>Total</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@(item.ElementAt(0))</td>
                <td>@(item.ElementAt(1))</td>
                <td>@(item.ElementAt(2))</td>
            </tr>
        }
    </tbody>
</table>